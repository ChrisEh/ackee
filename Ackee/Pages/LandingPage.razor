@using Ackee.Data
@using Microsoft.Extensions.DependencyInjection
@using Microsoft.EntityFrameworkCore;

    <main class="mainLayout_main__maxWidth landingPage">
        <div class="landingPage_information">
            <h1><b>Ackee unites your projects!</b></h1>
            <h4>
                A new way to manage your project <span style="color:Green;"><b>better</b></span>
            </h4>
            <p>Provides an overview of current projects to students and stakeholders:</p>
            <ul>
                <li>Time management</li>
                <li>Task management</li>
                <li>Project member management</li>
            </ul>

            <NavLink class="btn btn-primary align-self-center" href="identity/account/register" Match="NavLinkMatch.All">
                Join Ackee!
            </NavLink>
        </div>
        <div class="landingPage_image">
            <img src="img/dashboard_lp.PNG" alt="Example pages, for no reason at all." />
        </div>
    </main>

